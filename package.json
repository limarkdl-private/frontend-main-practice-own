{
  "name": "frontend-main-practice-own",
  "version": "1.1.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env port=3000 --env mode=development",
    "start:prod": "webpack serve --env port=3000 --env mode=production",
    "start:dev:server": "node ./json-server/index.js",
    "build": "webpack --env mode=production",
    "build:prod:Github": "webpack --env mode=production --env basenameMode=GithubPages",
    "build:prod:Vesta": "webpack --env mode=production --env basenameMode=Vesta",
    "build:prod:Xampp": "webpack --env mode=production --env basenameMode=Xampp",
    "build:dev": "webpack --env mode=development",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:css": "npx stylelint \"**/*.css\"",
    "lint:css:fix": "npx stylelint \"**/*.css\" --fix",
    "unit": "jest --config ./config/jest/jest.config.ts",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook",
    "pre-commit:test": "npm run lint:css:fix && npm run lint:ts:fix",
    "test:ui": "loki test",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/limarkdl/frontend-main-practice-own.git"
  },
  "keywords": [],
  "author": "limarkdl(Ivan Kostin)",
